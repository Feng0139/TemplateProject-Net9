{
  "ReleaseVersion": "1.0.0",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Properties": {
      "Application": "TemplateProject.Api"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://localhost:5341",
          "ApiKey": ""
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Mysql": "server=localhost;userid=root;password=123456;database=db_template_project;Allow User Variables=True;",
    "Redis": "localhost:6379,password=,ssl=False,abortConnect=False,syncTimeout=50000,allowAdmin=true"
  },
  "Cache": {
    "UseStorage": "Redis",
    "RedisDatabaseIndex": 0
  },
  "Hangfire": {
    "EnableDashboard": true,
    "DashboardPath": "/hangfire",
    "DashboardUser": "admin",
    "DashboardPassword": "hangfire"
  },
  "SecretKey": "PLEASE_ENTER_32_CHAR_SECRET_KEY_",
  "EnableScalar": true
}